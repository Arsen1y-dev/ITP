#
## ЕГЭ С2-2
#
#

n, m, k = map(int, input().split())
numbers = list(map(int, input().split()))


sum_result = 0


for num in numbers:
    if str(num)[-1] == str(k) and num % m != 0:
        sum_result += num


print(sum_result)
